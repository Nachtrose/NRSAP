{"name":"Drift-travel-with-tiles-Hex ( autoupdate )","type":"script","author":"YDfnQ8kHYo8L2nEG","img":"modules/NRSAP/packs/tiles-icon-drift-au.webp","scope":"global","command":"const compediumName = 'NRSAP.nrsap-space-travel';\nmacroRun('Drift-travel-with-tiles-Hex', compediumName);\n\nasync function macroRun(macroName, compendiumName='NRSAP.nrsap-space-travel') {  \n  let pack = game.packs.get(compendiumName);\n  let macro = ( await pack.getDocuments() ).find(i => (i.data.name==macroName) );\n  await macro.execute();    \n}","folder":null,"sort":0,"permission":{"default":0,"YDfnQ8kHYo8L2nEG":3},"flags":{"core":{"sourceId":"Macro.O98MLObOFyIPm27M"}},"_id":"BdTYN4cNjPb3Ass2"}
{"name":"Hyperspace-with-tiles","type":"script","author":"t7vzidlcIBBydvuk","img":"modules/NRSAP/packs/tiles-icon-hyperspace.webp","scope":"global","command":"// To work properly, this macro requires the module Sequencer as\n// a dependency.\nconst basePath = \"modules/NRSAP/library/travel/hyperspace\";\n\nconst loopingBackground = canvas.scene.tiles.find(tileDoc => tileDoc.texture.src.endsWith(\"loop.webm\"));\nconst coreBackground = canvas.scene.tiles.find(tileDoc => tileDoc.getFlag(\"world\", \"isNrsapBackgroundTile\"));\n\nif (!coreBackground) {\n   ui.notifications.info(\"No Tile flagged as background for this scene.\");\n   return;\n}\n\nif(!coreBackground || !loopingBackground) return;\n\nif(loopingBackground.data.alpha === 0.0){\n    await new FilePicker({\n        type: \"imagevideo\",\n        displayMode: \"tiles\",\n        current: coreBackground.texture.src,\n        callback: imagePath => { playSequence(imagePath) }\n    }).browse(`${basePath}/backgrounds/`);\n}else{\n    playSequence();\n}\n\nfunction playSequence(img){\n    \n    if(img){\n\n        new Sequence()\n            .sound(`${basePath}/sounds/FTLSound-Intro.ogg`)\n                .volume(0.55)\n            .wait(1850)\n            .effect(`${basePath}/hyperspace-start.webm`)\n                .atLocation(loopingBackground)\n\n            .wait(1300)\n            .animation()\n                .on(loopingBackground)\n                .opacity(1.0)\n                .volume(0.55)\n            .wait(1500)\n            .thenDo(() => { \n                coreBackground.update({ img: img });\n            })\n            .play()\n\n    }else{\n\n        new Sequence()\n            .sound(`${basePath}/sounds/FTLSound-Outro.ogg`)\n                .volume(0.55)\n            .wait(150)\n            .effect(`${basePath}/hyperspace-stop.webm`)\n                .atLocation(loopingBackground)\n            .wait(350)\n            .animation()\n                .on(loopingBackground)\n                .opacity(0.0)\n                .volume(0.0)\n            .play()\n\n    }\n\n}","flags":{"advanced-macros":{"runAsGM":false},"core":{"sourceId":"Macro.RfWYCJCK13Cog2Zl"}},"ownership":{"default":0,"YDfnQ8kHYo8L2nEG":3,"t7vzidlcIBBydvuk":3},"_stats":{"systemId":"sfrpg","systemVersion":"0.20.1","coreVersion":"10.288","createdTime":1665943832704,"modifiedTime":1666440161870,"lastModifiedBy":"t7vzidlcIBBydvuk"},"folder":null,"sort":0,"_id":"EPVfWLpXnTgSEcpB"}
{"_id":"Oy7E4cEfsmxTXWnz","name":"NRSAP-Drift-Travel-Shards ( autoupdate )","type":"script","author":"YDfnQ8kHYo8L2nEG","img":"modules/NRSAP/packs/drift-icon-au.webp","scope":"global","command":"const compediumName = 'NRSAP.nrsap-space-travel';\nmacroRun('NRSAP-Drift-Travel-Shards', compediumName);\n\nasync function macroRun(macroName, compendiumName='NRSAP.nrsap-space-travel') {  \n  let pack = game.packs.get(compendiumName);\n  let macro = ( await pack.getDocuments() ).find(i => (i.data.name==macroName) );\n  await macro.execute();    \n}","folder":null,"sort":0,"permission":{"default":0,"YDfnQ8kHYo8L2nEG":3},"flags":{"core":{"sourceId":"Macro.YAre3c7L80fZQoTo"}}}
{"name":"Drift-travel-with-tiles-Shards","type":"script","author":"t7vzidlcIBBydvuk","img":"modules/NRSAP/packs/tiles-icon-drift.webp","scope":"global","command":"// To work properly, this macro requires the module Sequencer as\n// a dependency.\nconst basePath = \"modules/NRSAP/library/travel/hyperspace\";\n\nconst loopingBackground = canvas.scene.tiles.find(tileDoc => tileDoc.texture.src.endsWith(\"loop.webm\"));\nconst coreBackground = canvas.scene.tiles.find(tileDoc => tileDoc.getFlag(\"world\", \"isNrsapBackgroundTile\"));\n\nif (!coreBackground) {\n   ui.notifications.info(\"No Tile flagged as background for this scene.\");\n   return;\n}\n\nif(!coreBackground || !loopingBackground) return;\n\nif(loopingBackground.data.alpha === 0.0){\n    await new FilePicker({\n        type: \"imagevideo\",\n        displayMode: \"tiles\",\n        current: coreBackground.texture.src,\n        callback: imagePath => { playSequence(imagePath) }\n    }).browse(`${basePath}/backgrounds/`);\n}else{\n    playSequence();\n}\n\nfunction playSequence(img){\n    \n    if(img){\n\n        new Sequence()\n            .sound(`${basePath}/sounds/drift-entry-shard.ogg`)\n                .volume(0.55)\n            .wait(2100)\n            .effect(`${basePath}/drift-entry-shard.webm`)\n                .atLocation(loopingBackground)\n                .fadeOut(2000, {ease: \"easeOutCubic\", delay: 1000})\n            .wait(2050)\n            .animation()\n                .on(loopingBackground)\n                .opacity(1.0)\n                .volume(0.55)\n            .wait(1500)\n            .thenDo(() => { \n                coreBackground.update({ img: img });\n            })\n            .play()\n\n    }else{\n\n        new Sequence()\n            .sound(`${basePath}/sounds/drift-exit-shard.ogg`)\n                .volume(0.55)\n            .wait(750)\n            .effect(`${basePath}/drift-exit-shard.webm`)\n                .atLocation(loopingBackground)\n            .wait(250)\n            .animation()\n                .on(loopingBackground)\n                .opacity(0.0)\n                .volume(0.0)\n            .play()\n\n    }\n\n}","flags":{"core":{"sourceId":"Macro.9POfPGVhDx7rOqoM"}},"ownership":{"default":0,"YDfnQ8kHYo8L2nEG":3,"t7vzidlcIBBydvuk":3},"_stats":{"systemId":"sfrpg","systemVersion":"0.20.1","coreVersion":"10.288","createdTime":1665943824378,"modifiedTime":1666440147888,"lastModifiedBy":"t7vzidlcIBBydvuk"},"folder":null,"sort":0,"_id":"P6f4xWCea0d8bX8k"}
